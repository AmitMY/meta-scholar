{
  "name": "SLCrawl",
  "descriptions": "Crawling online sources for sign-language videos",
  "data": {
    "id": {
      "type": "text"
    },
    "texts": {
      "type": "list",
      "parameters": {
        "type": "object",
        "parameters": {
          "text": {
            "type": "text"
          },
          "modifier": {
            "type": "text"
          }
        }
      }
    },
    "gloss": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "video_url": {
      "type": "url"
    },
    "video": {
      "type": "file",
      "parameters": {
        "type": "video"
      }
    },
    "timing": {
      "type": "object",
      "parameters": {
        "start": {
          "type": "number",
          "end": "number"
        }
      }
    },
    "sign_language": {
      "type": "text"
    },
    "text_language": {
      "type": "text"
    },
    "signer": {
      "type": "object",
      "parameters": {
        "gender": {
          "type": "text"
        },
        "age": {
          "type": "number"
        },
        "ethnicity": {
          "type": "text"
        },
        "synthetic": {
          "type": "boolean"
        }
      }
    }
  },
  "versions": [
    {
      "version": "HandSpeak",
      "url": "https://www.handspeak.com/"
    },
    {
      "version": "FingerSpell",
      "url": "https://fingerspell.net/static/sprites/"
    },
    {
      "version": "SpreadTheSign"
    },
    {
      "version": "ASLChars",
      "url": "https://github.com/mon95/Sign-Language-and-Static-gesture-recognition-using-sklearn/raw/master/Dataset.zip"
    }
  ]
}